{
  "name": "camunda-modeler-plugin-multidiagram",
  "version": "2.0.0",
  "description": "The Camunda Modeler multidiagram plug-in",
  "keywords": [
    "camunda",
    "modeler",
    "plugin",
    "multi",
    "diagram",
    "multi-diagram"
  ],
  "main": "index.js",
  "scripts": {
    "all": "run-s build build:serve",
    "build": "webpack",
    "build:serve": "webpack --config webpack.config.serve.js",
    "start": "run-s build:serve serve",
    "dev": "run-p \"build:serve -- --watch\" serve",
    "serve": "sirv public --dev",
    "lint": "eslint . --fix",
    "test": "run-s lint all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sharedchains/camunda-modeler-plugin-multidiagram.git"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "camunda-modeler-plugin-helpers": "^5.1.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "eslint": "^7.32.0",
    "eslint-plugin-bpmn-io": "^0.13.0",
    "eslint-plugin-react": "^7.29.4",
    "file-loader": "^6.2.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "react-svg-loader": "^3.0.3",
    "sirv-cli": "^2.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-sources": "^3.2.3",
    "zip-webpack-plugin": "^4.0.1"
  },
  "dependencies": {
    "@bpmn-io/properties-panel": "^0.13.2",
    "bpmn-js": "^9.0.3",
    "bpmn-js-properties-panel": "^1.0.0",
    "camunda-bpmn-moddle": "^6.1.2",
    "classnames": "^2.3.1",
    "jquery": "^3.6.0",
    "min-dash": "^3.8.1",
    "min-dom": "^3.1.3"
  }
}
